{
  "name": "career-data-api",
  "version": "1.0.0",
  "description": "GraphQL API for career data management with AI-powered document generation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node scripts/bundle.js",
    "build:check": "tsc --noEmit && node scripts/bundle.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "graphql",
    "career",
    "resume",
    "ai",
    "anthropic"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@fastify/aws-lambda": "^5.0.0",
    "@fastify/cors": "^10.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.0.0",
    "graphql": "^16.9.0",
    "mercurius": "^15.0.0",
    "mongodb": "^6.12.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.2",
    "fast-check": "^3.23.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
